{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "plugins": [
        "react"
    ],
    "parser": "babel-eslint",
    "rules": {
        "max-len": [
            "error",
            120
        ],
        "indent": [
            "error",
            4
        ],
        "class-methods-use-this": "off",
        "no-console": "warn"
    }
}